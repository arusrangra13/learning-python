
#function defination
def cal_sum(a,b): #parameters
    return a+b

sum = cal_sum(1,6) #function call; arguments
print(sum)


#example
def print_Nameste():
    print("Nameste")

print_Nameste()
print_Nameste()
print_Nameste()
print_Nameste()
print_Nameste()

#marks avg
def marks_avg(a,b,c,d,e,f):
    sum=a+b+c+d+e+f
    avg=sum/6
    print(avg)
    return avg

marks_avg(39,31,46,40,47,44)
